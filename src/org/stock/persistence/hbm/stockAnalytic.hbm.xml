<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.stock.persistence.domain.StockAnalytic" table="stock_analytic">
        <id name="stockName" type="java.lang.String">
			<column name="stockName" />
		</id>

		<property name="volume" type="java.lang.Integer">
			<column name="volume" />
		</property>
		<property name="totalVolume" type="java.lang.Integer">
			<column name="totalVolume" />
		</property>
		<property name="relative" type="java.lang.Double">
			<column name="relative" />
		</property>
		<property name="relativePlus" type="java.lang.Double">
			<column name="relativePlus" />
		</property>
		<property name="riseDay" type="java.lang.Integer">
			<column name="riseDay" />
		</property>
		<property name="riseDayPlus" type="java.lang.Double">
			<column name="riseDayPlus" />
		</property>
		<property name="analyticDay" type="java.lang.Integer">
			<column name="analyticDay" />
		</property>
    </class>
</hibernate-mapping>
